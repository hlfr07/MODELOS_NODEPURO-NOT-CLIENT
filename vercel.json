{
  "version": 2,
  "builds": [
    {
      "src": "main.serverless.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.serverless.ts",
      "methods": ["GET","POST","PUT","DELETE","PATCH","OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
